{
  "name": "@coblocks-stage/core",
  "version": "0.3.0",
  "description": "Stage lib for coblocks",
  "author": "BlueNyang",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "types": "dist/index.d.ts",
