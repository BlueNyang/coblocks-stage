    "@vitest/ui": "^3.2.4",
    "jsdom": "^26.1.0",
    "rollup": "^4.45.1",
    "rollup-plugin-dts": "^6.2.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "license": "MIT",
